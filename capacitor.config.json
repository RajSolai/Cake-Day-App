{
  "appId": "rf.gd.solairaj.cakedays",
  "appName": "Cake Days",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "cordova": {
    "preferences": {
      "ScrollEnabled": "false",
      "android-minSdkVersion": "19",
